{
    "name": "lumina-mongoose",
    "description": "A lumina extension that adds predefined methods for fetching entities from route parameters in a Restify application.",
    "version": "0.0.1",
    "keywords": [
        "lumina",
        "mongoose",
        "mongodb",
        "restify"
    ],
    "repository": { 
        "type": "git",
        "url": "https://github.com/Eagerod/lumina-mongoose.git"
    },
    "dependencies": {
        "mongoose": "*",
        "restify": "*",
        "async": "1.4.0"
    },
    "devDependencies": {
        "request" : "2.55.0",
        "lumina": "0.0.5",
        "jsdoc": "3.3.2",
        "eslint": "0.24.1",
        "nodeunit": "0.9.1",
        "istanbul": "0.3.17"
    },
    "author": {
        "name": "Aleem Haji",
        "author": "hajial@gmail.com"
    },
    "scripts": {
        "test": "./node_modules/.bin/nodeunit tests/test.js",
        "docs": "./node_modules/.bin/jsdoc --recurse ./lib -d docs",
        "lint": "./node_modules/.bin/eslint .",
        "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/nodeunit tests/test.js"
    }
}
